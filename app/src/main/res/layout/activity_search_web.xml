<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.omg.ireader.ui.activity.SearchWebActivity">

    <WebView
        android:id="@+id/search_web_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></WebView>


    <ProgressBar
        android:id="@+id/mProgressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="2dip"
        android:progressDrawable="@drawable/progress_bar_horizontal" />
    <com.omg.ireader.widget.WebSearchOnTouchView
        android:id="@+id/touch_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.omg.ireader.widget.WebSearchOnTouchView>
    <com.omg.ireader.widget.view.menu.AllAngleExpandableButton
        android:id="@+id/web_button_expandable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|right"
        app:aebAnimDurationMillis="175"
        app:aebButtonElevation="4dp"
        app:aebButtonGapDp="45dp"
        app:aebEndAngleDegree="180"
        app:aebIsSelectionMode="false"
        app:aebMainButtonRotateAnimDurationMillis="300"
        app:aebMainButtonRotateDegree="-135"
        app:aebMainButtonSizeDp="45dp"
        app:aebRippleEffect="true"
        android:visibility="gone"
        app:aebStartAngleDegree="90"
        app:aebSubButtonSizeDp="45dp" />

</FrameLayout>
